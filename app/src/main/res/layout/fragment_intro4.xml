<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Intro4Fragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
                <ImageView
                    android:id="@+id/Intro4Image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/IntroDone"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/intro4"
                    />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/IntroDone"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:text="시작하기"
                    android:textColor="@color/tenantList_date_background"
                    android:textSize="25sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:backgroundTint="@color/main_button_background"
                    android:background="@drawable/del_button_shape"
                    app:layout_constraintBottom_toBottomOf="@id/Intro4Image"
                    android:layout_marginBottom="90dp"
                    android:layout_marginRight="20dp"
                    />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>